@{
    ViewBag.Title = "Home Page";
}
@model List<labtask1.Models.student>

<div class="container">
    <table class="table table-condensed">
        <tr>
            <th>ID</th>
            <th>NAME</th>
            <th>CGPA</th>
            <th></th>
            <th></th>
        </tr>
        @foreach (var student in @Model)
        {
            <tr>
                <td>@student.ID</td>
                <td>@student.NAME</td>
                <td>@student.CGPA</td>
                <td><a class="btn btn-success"href="/student/Edit/@student.ID">Edit</a></td>
                <td><a class="btn btn-danger"href="/student/delete/@student.ID">delete</a></td>
                <th></th>

            </tr>

        }
      
    </table>
</div>